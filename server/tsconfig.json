{
  "include": [
    "src",
    "../client/src/shared"
  ],
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "..",
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ]
  }
}
